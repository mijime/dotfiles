{
  "suggest.autoTrigger": "none",
  "coc.preferences.formatOnSaveFiletypes": [
    "*"
  ],
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": [
        "serve"
      ],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {},
      "settings": {}
    },
    "go": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".git",
        ".vim"
      ],
      "filetypes": [
        "go"
      ]
    }
  }
}
